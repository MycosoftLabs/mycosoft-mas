# Database Settings
DATABASE_URL=sqlite:///data/mycosoft.db
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# Redis Settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=False

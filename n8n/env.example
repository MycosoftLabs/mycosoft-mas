# MYCA n8n Integration Fabric Environment Configuration
# Copy this file to .env and fill in your values

# n8n Basic Auth
N8N_USER=admin
N8N_PASSWORD=changeme_secure_password

# Database
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=changeme_secure_postgres_password

# Redis
REDIS_PASSWORD=changeme_secure_redis_password

# n8n Settings
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_WEBHOOK_URL=http://localhost:5678
TIMEZONE=America/New_York

# Vault (optional)
VAULT_ADDR=http://vault:8200
VAULT_TOKEN=

# Logging
N8N_LOG_LEVEL=info

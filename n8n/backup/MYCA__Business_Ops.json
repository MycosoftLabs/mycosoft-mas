                  {
                      "parameters":  {
                                         "jsCode":  "// Format business response\nconst input = $input.first().json;\n\nreturn {\n  json: {\n    status: \u0027success\u0027,\n    domain: input.domain,\n    operation: input.operation,\n    result: input.result,\n    data: input,\n    timestamp: new Date().toISOString()\n  }\n};"
                                     },

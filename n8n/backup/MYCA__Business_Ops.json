                  {
                      "parameters":  {
                                         "jsCode":  "// Parse business operations request\nconst input = $input.first().json;\n\nconst action = input.action || \u0027general\u0027;\nconst message = input.message || \u0027\u0027;\nconst sessionId = input.session_id || `biz_${Date.now()}`;\n\n// Determine business domain\nlet domain = \u0027general\u0027;\nif (/task|todo|project|asana|jira|sprint|ticket|backlog/i.test(message)) domain = \u0027project_management\u0027;\nelse if (/crm|customer|client|lead|sales|deal|pipeline/i.test(message)) domain = \u0027crm\u0027;\nelse if (/report|analytics|metrics|kpi|dashboard|stats/i.test(message)) domain = \u0027reporting\u0027;\nelse if (/notion|document|wiki|knowledge|doc/i.test(message)) domain = \u0027knowledge\u0027;\nelse if (/slack|discord|team|message|channel/i.test(message)) domain = \u0027communication\u0027;\nelse if (/invoice|payment|billing|expense|finance/i.test(message)) domain = \u0027finance\u0027;\nelse if (/hr|employee|team|hiring|onboard/i.test(message)) domain = \u0027hr\u0027;\n\n// Determine operation\nlet operation = \u0027query\u0027;\nif (/create|add|new|make/i.test(message)) operation = \u0027create\u0027;\nelse if (/update|edit|modify|change/i.test(message)) operation = \u0027update\u0027;\nelse if (/delete|remove|cancel/i.test(message)) operation = \u0027delete\u0027;\nelse if (/list|show|get|find|search/i.test(message)) operation = \u0027list\u0027;\nelse if (/assign|delegate|transfer/i.test(message)) operation = \u0027assign\u0027;\nelse if (/complete|done|finish|close/i.test(message)) operation = \u0027complete\u0027;\n\nreturn {\n  json: {\n    action: action,\n    domain: domain,\n    operation: operation,\n    message: message,\n    session_id: sessionId,\n    timestamp: new Date().toISOString()\n  }\n};"
                                     },

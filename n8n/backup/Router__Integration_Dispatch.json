                      "id":  "load-registry",
                      "name":  "Load Integration Registry",
                      "type":  "n8n-nodes-base.fs",
                      "typeVersion":  1,
                      "position":  [
                                       460,
                                       300
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// Parse registry and find integration\nconst command = $(\u0027Execute Workflow Trigger\u0027).item.json.command;\nconst registryFile = $input.item.json.data;\nconst registry = JSON.parse(registryFile.toString());\n\nconst integrationName = command.integration;\nlet integration = null;\n\n// If integration specified, find it\nif (integrationName) {\n  integration = registry.integrations.find(i =\u003e i.integration === integrationName);\n  \n  if (!integration) {\n    return [{\n      json: {\n        error: true,\n        status: 404,\n        message: `Integration \u0027${integrationName}\u0027 not found in registry`,\n        request_id: command.request_id\n      }\n    }];\n  }\n} else {\n  // Try to infer integration from intent using basic pattern matching\n  // This is a simple implementation - could be enhanced with ML/embeddings\n  const intent = (command.intent || \u0027\u0027).toLowerCase();\n  \n  // Basic keyword matching for common integrations\n  const keywords = {\n    \u0027sheet\u0027: \u0027google_sheets\u0027,\n    \u0027gmail\u0027: \u0027gmail\u0027,\n    \u0027email\u0027: \u0027gmail\u0027,\n    \u0027drive\u0027: \u0027google_drive\u0027,\n    \u0027calendar\u0027: \u0027google_calendar\u0027,\n    \u0027github\u0027: \u0027github\u0027,\n    \u0027jira\u0027: \u0027jira\u0027,\n    \u0027slack\u0027: \u0027slack\u0027,\n    \u0027telegram\u0027: \u0027telegram\u0027,\n    \u0027openai\u0027: \u0027openai\u0027,\n    \u0027proxmox\u0027: \u0027proxmox\u0027,\n    \u0027unifi\u0027: \u0027unifi\u0027\n  };\n  \n  for (const [keyword, intName] of Object.entries(keywords)) {\n    if (intent.includes(keyword)) {\n      integration = registry.integrations.find(i =\u003e i.integration === intName);\n      break;\n    }\n  }\n  \n  if (!integration) {\n    return [{\n      json: {\n        error: true,\n        status: 400,\n        message: \u0027Could not determine integration. Please specify integration name.\u0027,\n        request_id: command.request_id\n      }\n    }];\n  }\n}\n\n// Check if integration is enabled\nif (!integration.enabled) {\n  return [{\n    json: {\n      error: true,\n      status: 403,\n      message: `Integration \u0027${integration.integration}\u0027 is disabled`,\n      request_id: command.request_id\n    }\n  }];\n}\n\n// Determine routing\nconst routing = {\n  error: false,\n  command: command,\n  integration: integration,\n  category: integration.category,\n  use_native: integration.native_node === true,\n  confirm_required: integration.confirm_required === true,\n  risk_level: integration.risk,\n  request_id: command.request_id\n};\n\nreturn [{ json: routing }];"
                                     },

                  {
                      "parameters":  {
                                         "jsCode":  "// General fallback for unrouted requests\nconst input = $input.first().json;\n\nreturn {\n  json: {\n    result: `Tool request received: ${input.action}. The specific tool is not yet configured.`,\n    data: input,\n    action: input.action || \u0027unknown\u0027\n  }\n};"
                                     },

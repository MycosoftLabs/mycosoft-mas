                                       780,
                                       100
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// Weather API Integration\nconst input = $input.first().json;\nconst message = input.message;\n\n// Extract location from message\nlet location = \u0027local\u0027;\nconst locationMatch = message.match(/(?:weather|temperature|forecast).*?(?:in|at|for)\\s+([\\w\\s,]+)/i);\nif (locationMatch) {\n  location = locationMatch[1].trim();\n}\n\n// Weather API placeholder - Replace with OpenWeatherMap, WeatherAPI, etc.\nconst weatherData = {\n  location: location,\n  note: \u0027Configure OpenWeatherMap API credentials in n8n to get real weather data.\u0027,\n  suggestion: \u0027Add HTTP Request node with OpenWeatherMap API: https://api.openweathermap.org/data/2.5/weather\u0027\n};\n\nreturn {\n  json: {\n    result: `Weather information for ${location}. ${weatherData.note}`,\n    data: weatherData,\n    action: \u0027weather\u0027\n  }\n};"
                                     },
                      "id":  "weather",
                      "name":  "Weather",
                      "type":  "n8n-nodes-base.code",
                      "typeVersion":  2,
                      "position":  [
                                       780,
                                       240
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// Calendar Integration (Google Calendar / Outlook)\nconst input = $input.first().json;\nconst message = input.message;\nconst entities = input.entities || {};\n\n// Parse calendar intent\nlet calendarAction = \u0027list\u0027;\nif (/create|add|schedule|book/i.test(message)) calendarAction = \u0027create\u0027;\nelse if (/delete|remove|cancel/i.test(message)) calendarAction = \u0027delete\u0027;\nelse if (/update|change|modify|reschedule/i.test(message)) calendarAction = \u0027update\u0027;\n\n// Extract date/time from entities\nconst dates = entities.dates || [];\nconst times = entities.times || [];\n\nconst calendarData = {\n  action: calendarAction,\n  dates: dates,\n  times: times,\n  note: \u0027Connect Google Calendar or Microsoft Outlook credentials in n8n to manage calendar.\u0027,\n  suggestion: \u0027Add Google Calendar node with OAuth2 credentials.\u0027\n};\n\nreturn {\n  json: {\n    result: `Calendar ${calendarAction} request processed. ${calendarData.note}`,\n    data: calendarData,\n    action: \u0027calendar\u0027\n  }\n};"
                                     },
                      "id":  "calendar",
                      "name":  "Calendar",
                      "type":  "n8n-nodes-base.code",
                      "typeVersion":  2,
                      "position":  [
                                       780,
                                       380
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// Email Integration (Gmail / Outlook / SMTP)\nconst input = $input.first().json;\nconst message = input.message;\nconst entities = input.entities || {};\n\n// Parse email intent\nlet emailAction = \u0027read\u0027;\nif (/send|compose|write|reply/i.test(message)) emailAction = \u0027send\u0027;\nelse if (/read|check|inbox|unread/i.test(message)) emailAction = \u0027read\u0027;\nelse if (/search|find.*email/i.test(message)) emailAction = \u0027search\u0027;\n\n// Extract email addresses from entities\nconst emailAddresses = entities.emails || [];\n\nconst emailData = {\n  action: emailAction,\n  recipients: emailAddresses,\n  note: \u0027Connect Gmail or Microsoft Outlook credentials in n8n to manage email.\u0027,\n  suggestion: \u0027Add Gmail node with OAuth2 credentials.\u0027\n};\n\nreturn {\n  json: {\n    result: `Email ${emailAction} request processed. ${emailData.note}`,\n    data: emailData,\n    action: \u0027email\u0027\n  }\n};"
                                     },
                      "id":  "email",
                      "name":  "Email",
                      "type":  "n8n-nodes-base.code",
                      "typeVersion":  2,
                      "position":  [
                                       780,
                                       520
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// File Management\nconst input = $input.first().json;\nconst message = input.message;\n\n// Parse file intent\nlet fileAction = \u0027list\u0027;\nif (/create|new|make/i.test(message)) fileAction = \u0027create\u0027;\nelse if (/delete|remove/i.test(message)) fileAction = \u0027delete\u0027;\nelse if (/copy|duplicate/i.test(message)) fileAction = \u0027copy\u0027;\nelse if (/move|rename/i.test(message)) fileAction = \u0027move\u0027;\nelse if (/read|open|view|show/i.test(message)) fileAction = \u0027read\u0027;\nelse if (/list|find|search/i.test(message)) fileAction = \u0027list\u0027;\n\n// Extract path hints from message\nconst pathMatch = message.match(/[\\/\\\\][\\w\\-\\.\\/\\\\]+/g) || [];\n\nconst fileData = {\n  action: fileAction,\n  paths: pathMatch,\n  note: \u0027Configure file access through n8n local file nodes or NAS integration.\u0027,\n  suggestion: \u0027Add Read/Write File nodes or FTP/SFTP nodes for NAS access.\u0027\n};\n\nreturn {\n  json: {\n    result: `File ${fileAction} request processed. ${fileData.note}`,\n    data: fileData,\n    action: \u0027files\u0027\n  }\n};"
                                     },
                      "id":  "files",
                      "name":  "Files",
                      "type":  "n8n-nodes-base.code",
                      "typeVersion":  2,
                      "position":  [
                                       780,
                                       660
                                   ]
                  },
                  {
                      "parameters":  {
                                         "jsCode":  "// Code Execution / Development Tools\nconst input = $input.first().json;\nconst message = input.message;\n\n// Parse code intent\nlet codeAction = \u0027analyze\u0027;\nif (/run|execute|eval/i.test(message)) codeAction = \u0027execute\u0027;\nelse if (/debug|fix|error/i.test(message)) codeAction = \u0027debug\u0027;\nelse if (/deploy|push|release/i.test(message)) codeAction = \u0027deploy\u0027;\nelse if (/git|commit|branch/i.test(message)) codeAction = \u0027git\u0027;\nelse if (/generate|create.*code|write.*code/i.test(message)) codeAction = \u0027generate\u0027;\n\n// Extract code language hints\nlet language = \u0027python\u0027;\nif (/javascript|js|node/i.test(message)) language = \u0027javascript\u0027;\nelse if (/python|py/i.test(message)) language = \u0027python\u0027;\nelse if (/bash|shell|sh/i.test(message)) language = \u0027bash\u0027;\nelse if (/sql/i.test(message)) language = \u0027sql\u0027;\n\nconst codeData = {\n  action: codeAction,\n  language: language,\n  note: \u0027Code execution can be done through Execute Command node or remote API.\u0027,\n  suggestion: \u0027Add SSH node for remote execution or Execute Command for local scripts.\u0027\n};\n\nreturn {\n  json: {\n    result: `Code ${codeAction} (${language}) request processed. ${codeData.note}`,\n    data: codeData,\n    action: \u0027code\u0027\n  }\n};"
                                     },
                      "id":  "code",
                      "name":  "Code",
                      "type":  "n8n-nodes-base.code",

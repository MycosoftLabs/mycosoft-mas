# System Configuration
MAS_ENV=development
DEBUG_MODE=true
LOG_LEVEL=DEBUG
SECRET_KEY=dev_secret_key_change_in_prod

# Database
POSTGRES_USER=mas
POSTGRES_PASSWORD=maspassword
POSTGRES_DB=mas
DATABASE_URL=postgresql://mas:maspassword@postgres:5432/mas

# Redis
REDIS_URL=redis://redis:6379/0

# Qdrant
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# LLM Configuration
# Options: openai, azure, gemini, openai_compatible (for local/litellm)
LLM_DEFAULT_PROVIDER=openai_compatible
LLM_API_KEY=sk-1234
LLM_BASE_URL=http://litellm:4000

# Model Selection
LLM_MODEL_PLANNING=gpt-4
LLM_MODEL_EXECUTION=gpt-3.5-turbo
LLM_MODEL_FAST=gpt-3.5-turbo

# Grafana
GF_SECURITY_ADMIN_PASSWORD=admin

# Integration Secrets (Add your own)
# Google APIs
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Other integrations
STRIPE_API_KEY=...

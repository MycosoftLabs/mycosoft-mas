echo.
echo ========================================
echo    MYCOSOFT SYSTEM STARTUP
echo ========================================
echo.

:: Check if Docker Desktop is running
echo [1/5] Checking Docker Desktop...
docker info >nul 2>&1
if %ERRORLEVEL% neq 0 (
    echo [!] Docker Desktop is not running. Starting it...
    start "" "C:\Program Files\Docker\Docker\Docker Desktop.exe"
    echo [*] Waiting 30 seconds for Docker to initialize...
    timeout /t 30 /nobreak >nul
) else (
    echo [+] Docker Desktop is running
)

:: Start Docker containers
echo.
echo [2/5] Starting Docker containers...
cd /d "C:\Users\admin2\Desktop\MYCOSOFT\CODE\MAS\mycosoft-mas"
docker compose up -d
echo [+] Docker containers started

:: Wait for containers to be healthy
echo.
echo [3/5] Waiting for containers to be healthy...
timeout /t 10 /nobreak >nul

:: Show container status
echo.
echo [4/5] Container Status:
echo ----------------------------------------
docker ps --format "table {{.Names}}\t{{.Status}}\t{{.Ports}}" | findstr /V "NAMES"
echo ----------------------------------------

:: Start Website Dev Server
echo.
echo [5/5] Starting Mycosoft Website...
start "Mycosoft Website" cmd /k "cd /d C:\Users\admin2\Desktop\MYCOSOFT\CODE\WEBSITE\website && npm run dev"

echo.
echo ========================================
echo    MYCOSOFT SYSTEM IS STARTING
echo ========================================
echo.
echo Services Available:
echo   - Website:        http://localhost:3000
echo   - N8N Workflows:  http://localhost:5678
echo   - MAS Dashboard:  http://localhost:3100
echo   - MINDEX API:     http://localhost:8000
echo   - Myca App:       http://localhost:3001
echo   - MAS API:        http://localhost:8001
echo.
echo Docker Services (via docker ps):
echo   - Postgres:       localhost:5433
echo   - Redis:          localhost:6390
echo   - Qdrant:         localhost:6345
echo   - Whisper STT:    localhost:8765
echo   - TTS:            localhost:5500
echo.
echo Press any key to open the website in your browser...
pause >nul

start "" "http://localhost:3000"





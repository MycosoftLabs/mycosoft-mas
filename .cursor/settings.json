{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/out/**": true,
    "**/coverage/**": true,
    "**/venv*/**": true,
    "**/.venv/**": true,
    "**/venv/**": true,
    "**/ENV/**": true,
    "**/__pycache__/**": true,
    "**/.pytest_cache/**": true,
    "**/htmlcov/**": true,
    "**/.mypy_cache/**": true,
    "**/.tox/**": true,
    "**/logs/**": true,
    "**/*.log": true,
    "**/data/**": true,
    "**/firmware/**": true,
    "**/docker/**": true,
    "**/grafana/**": true,
    "**/prometheus/**": true,
    "**/redis/**": true,
    "**/n8n/**": true,
    "**/mycosoft_mas/**": true,
    "**/orchestrator-myca/**": true,
    "**/unifi-dashboard/**": true,
    "**/WEBSITE/**": true,
    "**/infra/**": true,
    "**/migrations/**": true,
    "**/models/**": true,
    "**/contracts/**": true,
    "**/services/**": true,
    "**/agents/**": true,
    "**/.cache/**": true,
    "**/tsconfig.tsbuildinfo": true
  },
  "files.exclude": {
    "**/.git": false,
    "**/node_modules": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/venv*": true,
    "**/logs": true,
    "**/data": true,
    "**/firmware": true,
    "**/docker": true,
    "**/grafana": true,
    "**/prometheus": true,
    "**/redis": true,
    "**/n8n": true,
    "**/mycosoft_mas": true,
    "**/orchestrator-myca": true,
    "**/unifi-dashboard": true,
    "**/WEBSITE": true,
    "**/infra": true,
    "**/tsconfig.tsbuildinfo": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.tsserver.watchOptions": {
    "excludeDirectories": [
      "**/node_modules",
      "**/.next",
      "**/dist",
      "**/build",
      "**/out",
      "**/venv*",
      "**/.venv",
      "**/logs",
      "**/data",
      "**/firmware",
      "**/docker",
      "**/grafana",
      "**/prometheus",
      "**/redis",
      "**/n8n",
      "**/mycosoft_mas",
      "**/orchestrator-myca",
      "**/unifi-dashboard",
      "**/WEBSITE",
      "**/infra"
    ]
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "typescript.suggest.autoImports": false,
  "typescript.preferences.quoteStyle": "single",
  "editor.maxTokenizationLineLength": 20000,
  "files.maxMemoryForLargeFilesMB": 4096
}


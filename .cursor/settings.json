{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/out/**": true,
    "**/coverage/**": true,
    "**/venv*/**": true,
    "**/.venv/**": true,
    "**/venv/**": true,
    "**/venv311/**": true,
    "**/ENV/**": true,
    "**/__pycache__/**": true,
    "**/.pytest_cache/**": true,
    "**/htmlcov/**": true,
    "**/.mypy_cache/**": true,
    "**/.tox/**": true,
    "**/logs/**": true,
    "**/*.log": true,
    "**/data/**": true,
    "**/firmware/**": true,
    "**/docker/**": true,
    "**/grafana/**": true,
    "**/prometheus/**": true,
    "**/redis/**": true,
    "**/n8n/**": true,
    "**/mycosoft_mas/**": true,
    "**/orchestrator-myca/**": true,
    "**/unifi-dashboard/**": true,
    "**/WEBSITE/**": true,
    "**/infra/**": true,
    "**/migrations/**": true,
    "**/models/**": true,
    "**/contracts/**": true,
    "**/services/**": true,
    "**/agents/**": true,
    "**/plugins/**": true,
    "**/secrets/**": true,
    "**/speech/**": true,
    "**/static/**": true,
    "**/.cache/**": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/*.mp3": true,
    "**/*.mp4": true,
    "**/*.bin": true,
    "**/*_test.json": true,
    "**/*_req.json": true,
    "**/comprehensive_test_report_*.json": true
  },
  "files.exclude": {
    "**/.git": false,
    "**/node_modules": false
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/venv*": true,
    "**/venv311": true,
    "**/logs": true,
    "**/data": true,
    "**/firmware": true,
    "**/docker": true,
    "**/grafana": true,
    "**/prometheus": true,
    "**/redis": true,
    "**/n8n": true,
    "**/mycosoft_mas": true,
    "**/orchestrator-myca": true,
    "**/unifi-dashboard": true,
    "**/WEBSITE": true,
    "**/infra": true,
    "**/migrations": true,
    "**/models": true,
    "**/contracts": true,
    "**/services": true,
    "**/agents": true,
    "**/plugins": true,
    "**/secrets": true,
    "**/speech": true,
    "**/static": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.tox": true,
    "**/htmlcov": true,
    "**/tsconfig.tsbuildinfo": true,
    "**/*.mp3": true,
    "**/*.mp4": true,
    "**/*.bin": true,
    "**/*_test.json": true,
    "**/*_req.json": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.tsserver.watchOptions": {
    "excludeDirectories": [
      "**/node_modules",
      "**/.next",
      "**/dist",
      "**/build",
      "**/out",
      "**/venv*",
      "**/.venv",
      "**/venv311",
      "**/logs",
      "**/data",
      "**/firmware",
      "**/docker",
      "**/grafana",
      "**/prometheus",
      "**/redis",
      "**/n8n",
      "**/mycosoft_mas",
      "**/orchestrator-myca",
      "**/unifi-dashboard",
      "**/WEBSITE",
      "**/infra",
      "**/migrations",
      "**/models",
      "**/contracts",
      "**/services",
      "**/agents",
      "**/plugins",
      "**/secrets",
      "**/speech",
      "**/static",
      "**/__pycache__",
      "**/.pytest_cache",
      "**/.mypy_cache",
      "**/.tox",
      "**/htmlcov",
      "**/.cache"
    ]
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "typescript.suggest.autoImports": false,
  "typescript.preferences.quoteStyle": "single",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.fixAll": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.maxTokenizationLineLength": 20000,
  "files.maxMemoryForLargeFilesMB": 4096,
  "files.exclude": {
    "**/.git": false,
    "**/node_modules": false,
    "**/venv311": true,
    "**/mycosoft_mas": true,
    "**/orchestrator-myca": true,
    "**/data": true,
    "**/logs": true,
    "**/*.log": true,
    "**/*.mp3": true,
    "**/*.mp4": true,
    "**/*.bin": true
  },
  "cursor.chat.maxContextLength": 50000,
  "cursor.general.enableIndexing": true,
  "cursor.general.indexingExcludePatterns": [
    "**/node_modules/**",
    "**/.next/**",
    "**/venv*/**",
    "**/mycosoft_mas/**",
    "**/orchestrator-myca/**",
    "**/data/**",
    "**/logs/**",
    "**/*.log",
    "**/*.mp3",
    "**/*.mp4",
    "**/*.bin"
  ],
  "plugins": {
    "slack": {
      "enabled": true
    },
    "notion-workspace": {
      "enabled": true
    },
    "functions": {
      "enabled": true
    },
    "supabase": {
      "enabled": true
    },
    "browserstack": {
      "enabled": true
    },
    "linear": {
      "enabled": true
    },
    "sentry": {
      "enabled": true
    },
    "cloudflare": {
      "enabled": true
    }
  }
}

